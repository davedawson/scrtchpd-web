    "babel-runtime": "5.6.15",
    "coveralls": "2.11.2",
    "css-loader": "0.14.4",
    "eslint": "0.24.0",
    "eslint-loader": "0.14.1",
    "eslint-plugin-react": "2.6.3",
    "jest-cli": "0.4.13",
    "jsx-loader": "0.13.2",
    "node-libs-browser": "0.5.2",
